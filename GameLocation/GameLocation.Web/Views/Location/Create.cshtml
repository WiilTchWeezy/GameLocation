
@{
    Layout = null;
}

<div class="row" ng-controller="LocationCreateController">
    <div class="col-sm-4"></div>
    <div class="col-sm-4">
        <form>
            <div class="margin-top">
                <div class="input-group">
                    <input type="number" class="form-control input-color" id="password" placeholder="Nome" ng-model="FriendId">
                </div>
                <div class="input-group">
                    <input type="number" class="form-control input-color" id="password" placeholder="Nome" ng-model="GameId">
                </div>
            </div>
            <button type="submit" ng-click="create();" prevent-default class="form-control btn btn-default margin-top button-default-color">Cadastrar</button>
        </form>
    </div>
    <div class="col-sm-4"></div>
</div>
