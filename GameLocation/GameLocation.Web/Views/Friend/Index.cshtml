<div ng-controller="FriendController">
    <div>
        <a href="#/friend/create"><i class="fa fa-plus-circle"></i> Adicionar</a>
    </div>
    <div class="row">
        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nome</th>
                    <th>Telefone</th>
                    <th>#</th>
                    <th>#</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="d in data">
                    <td>{{d.Id}}</td>
                    <td>{{d.Name}}</td>
                    <td>{{d.Phone}}</td>
                    <td><a href="#/friend/edit"><i class="fa fa-pencil"></i></a></td>
                    <td><a prevent-default ng-click="delete(d.Id);"><i class="fa fa-minus-circle"></i></a></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>